{
  "name": "yarn-pkg-config",
  "version": "0.29.5",
  "esy": {
    "build": [
      [
        "bash",
        "-c",
        "which pkg-config || (./esy/build.sh)"
      ]
    ],
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "/usr/lib/pkgconfig:/usr/local/lib/pkgconfig:/usr/share/pkgconfig:/usr/local/share/pkgconfig:$cur__lib:$PKG_CONFIG_PATH"
      }
    },
    "buildsInSource": "_build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
